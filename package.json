{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron electron/main.cjs",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron electron/main.cjs\"",
    "build-electron": "npm run build && electron-builder --config electron-builder.json",
    "dist": "npm run build && electron-builder --config electron-builder.json --publish=never",
    "pack": "npm run build && electron-builder --config electron-builder.json --dir",
    "build-installer": "node scripts/build-installer.js",
    "reset-passwords": "node remove-encryption.cjs"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.18",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.2.0",
    "date-fns": "^4.1.0",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.1",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.gym.dada",
    "productName": "GYM DADA",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}
